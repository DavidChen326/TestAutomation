<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Indebted Automation Testing" time-out="30000">
  <listeners>
  		<listener class-name="co.indebted.mypackage.utilities.IExecutionListenerTestNG"></listener>
  </listeners>
  
  <test thread-count="5" name="LogIn">
    <classes>
      <class name="co.indebted.mypackage.tests.general.LoginTest"/>
    </classes>
  </test> <!-- logIn -->
  
  <test thread-count="5" name="ResetPassword">
    <classes>
      <class name="co.indebted.mypackage.tests.general.ResetPasswordTest"/>
    </classes>
  </test> <!-- ResetPassword -->
  
  <test thread-count="5" name="debts">
    <classes>
      <class name="co.indebted.mypackage.tests.debts.AdjustmentTest"/>
    </classes>
  </test> <!-- debts -->
  
  <test thread-count="5" name="Analyse">
    <classes>
      <class name="co.indebted.mypackage.tests.analyse.AnalyseTest"/>
    </classes>
  </test> <!-- Analyse -->
  
  <test thread-count="5" name="Settings">
    <classes>
      <class name="co.indebted.mypackage.tests.settings.SettingsTest"/>
      <class name="co.indebted.mypackage.tests.settings.GeneralTabTest"/>
      <class name="co.indebted.mypackage.tests.settings.ComplianceTabTest"/>
      <class name="co.indebted.mypackage.tests.settings.RolesTabTest"/>
      <class name="co.indebted.mypackage.tests.settings.TeamsTabTest"/>
      <class name="co.indebted.mypackage.tests.settings.UsersTabTest"/>
      <class name="co.indebted.mypackage.tests.settings.AssistTabTest"/>
      <class name="co.indebted.mypackage.tests.settings.DebtsTabTest"/>
      <class name="co.indebted.mypackage.tests.settings.InboxTabTest"/>
      <class name="co.indebted.mypackage.tests.settings.BuildTabTest"/>
      <class name="co.indebted.mypackage.tests.settings.ContactsTabTest"/>
    </classes>
  </test> <!-- Settings -->
    
  <test thread-count="5" name="Portals">
    <classes>
      <class name="co.indebted.mypackage.tests.portals.AgentPortalTest"/>
      <class name="co.indebted.mypackage.tests.portals.DebtorPortalTest"/>
      <class name="co.indebted.mypackage.tests.portals.HardshipAgentTest"/>
    </classes>
  </test> <!-- Portals -->
  
</suite> <!-- Suite -->
